{
  "manifest_version": 3,
  "name": "Timer Extension",
  "version": "0.0.1",
  "description": "Chrome extension",
  "icons": {
    "16": "timer.png",
    "48": "timer.png",
    "128": "timer.png"
  },
  "action": {
    "default_icon": {
      "16": "timer.png",
      "24": "timer.png",
      "32": "timer.png"
    },
    "default_title": "Timer Extension Action Title",
    "default_popup": "index.html"
  },
  "options_page": "options.html",
  "permissions": [
    "storage"
  ]
}